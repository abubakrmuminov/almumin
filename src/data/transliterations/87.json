{
  "87:1": "sab-bih  is-ma  rab-bi-kal  a-laa",
  "87:2": "al-lazee  kha-la-qa  fa-saw-waa",
  "87:3": "wal-lazee   qad-da-ra  fa-ha-daa",
  "87:4": "wal-lazeee  ak-hra-jal  ma-raa",
  "87:5": "fa-jaa-la-hoo  ghu-saaaan  ah-waa",
  "87:6": "sa-nuq-riu-ka  fa-laa  tan-saaa",
  "87:7": "il-laa  maa  shaaaal   laah     in-na-hoo  ya-la-mul  jah-ra    wa-maa  yak-hfaa",
  "87:8": "wa-nuyas-si-ru-ka  li-lyus-raa",
  "87:9": "fa-zak-kir  in  na-faa-tiz  zik-raa",
  "87:10": "sayaz-zak-ka-ru  mai  yak-hshaa",
  "87:11": "waya-ta-jan-na-bu-hal  as-hqaa",
  "87:12": "al-lazee  yas-lan  naa-ral  kub-raa",
  "87:13": "sum-ma  laa  ya-moo-tu  fee-haa  wa-laa  ya-hyaa",
  "87:14": "qad  af-la-ha  man  ta-zak-kaa",
  "87:15": "wa-za-ka-ras  ma     rab-bihee  fa-sal-laa",
  "87:16": "bal  tu-si-roo-nal  hayaa-tad  du-nyaa",
  "87:17": "wa-laa-khi-ra-tu  khai-runw  waab-qaa",
  "87:18": "in-na  haa-zaa  la-fis  su-hu-fil  oo-laa",
  "87:19": "su-hu-fi  ib-raa-hee-ma  wa-moo-saa"
}
